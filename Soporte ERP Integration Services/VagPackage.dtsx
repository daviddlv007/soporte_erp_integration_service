                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    ud.codigo          AS unidad_negocio_id,
    ud.nombre          AS nombre,
    eg.nombre          AS grupo_empresarial
FROM erp_soporte.unidad_de_negocio ud
INNER JOIN erp_soporte.empresa_o_grupo eg
